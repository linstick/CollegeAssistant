<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.linstick.collegeassistant">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity
            android:name=".activities.MainActivity"
            android:theme="@style/MainActivityTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".activities.EditNoteActivity"
            android:label="添加帖子" />

        <activity
            android:name=".activities.SearchActivity"
            android:label="搜索">
        </activity>

        <activity
            android:name=".activities.NoteDetailActivity"
            android:label="详情">
        </activity>

        <activity
            android:name=".activities.UserInfoActivity"
            android:label="用户信息">
        </activity>

        <activity
            android:name=".activities.PersonalNotesActivity"
            android:label="我的帖子">
        </activity>

        <activity
            android:name=".activities.CollectionsActivity"
            android:label="我的收藏">
        </activity>

        <activity
            android:name=".activities.PersonalRelatedActivity"
            android:label="与我相关">
        </activity>

        <activity
            android:name=".activities.FeedBackActivity"
            android:label="用户反馈">
        </activity>

        <activity
            android:name=".activities.LoginActivity">

        </activity>

        <activity
            android:name=".activities.EditUserInfoActivity"
            android:label="编辑资料">
        </activity>

        <activity
            android:name=".activities.ModifyPasswordActivity"
            android:label="修改密码">
        </activity>


        <activity android:name=".activities.TestActivity">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>


    </application>

</manifest>